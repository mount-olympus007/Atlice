            </div>
        </div>
    }


</div>
@if(Model.TapLinks.Where(x => x.LinkType == Domain.Entities.LinkType.Connect || x.LinkType == Domain.Entities.LinkType.Pay).Count() > 10)
{
    <div class="w-col w-col-6">
    @foreach (var link in Model.TapLinks.Where(x => x.LinkType == Domain.Entities.LinkType.Connect || x.LinkType == Domain.Entities.LinkType.Pay).Skip(10))
    {
        <div class="friend-item">
            <div class="friend-photo-container">
                <img width="50" src="@link.GetLogo(link.SocialProvider)" alt="" class="friend-photo">
                <div class="online-status off"></div>
            </div>
            <div class="tap-links-form w-form">
                <form id="email-form-2" name="email-form-2" data-name="Email Form 2" method="get">
                    <div>
                        <div class="card no-padding">
                            <div class="card-setting-container">
                                <div class="card-setting-switcher">
                                    <label class="w-checkbox switcher-wrapper-checkbox">
