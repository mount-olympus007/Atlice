                            <form class="inventory-search-form" asp-page-handler="FindNote" method="post">
                                <label for="name-8" class="field-label-2">Last Name:</label>
                                <div class="div-block-5">
                                    <input type="text" class="text-field-3 w-input" maxlength="256" name="lastname" data-name="Name 7" placeholder="" id="name-7">
                                    <input type="submit" value="" data-wait="Please wait..." class="submit-button-2 search w-button">
                                </div>
                                <label for="name-8" class="field-label-2">Action:</label>
                                <div class="div-block-6">
                                    <select class="text-field-3 w-input" maxlength="256" name="action">
                                        <option value="sent email">Sent Email</option>
                                        <option value="new beta ask order">New Beta Ask Order</option>
                                        <option value="activated device">Activated Device</option>
                                        <option value="registered">Registered</option>
                                        <option value="agreed to terms">Agreed To Terms</option>
                                        <option value="invited">Invited</option>
                                        <option value="prospect found">Prospect Found</option>
                                        <option value="opt-out">Opt-Out of Beta Ask</option>
