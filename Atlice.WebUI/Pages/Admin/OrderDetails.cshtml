@page "{id}"

@model Atlice.WebUI.Pages.Admin.OrderDetailsModel
@{
    Layout = null;

}
<!DOCTYPE html>
<html data-wf-page="604ffe830866098287935a25" data-wf-site="60442746f919e95d6b31b806">
<head>
    <meta charset="utf-8">
    <title>Atlice</title>
    <meta content="Atlice" property="og:title">
    <meta content="Atlice" property="twitter:title">
    <meta content="width=device-width, initial-scale=1" name="viewport">
    <meta content="Webflow" name="generator">
    <link href="~/AA1.3/css/normalize.css" rel="stylesheet" type="text/css">
    <link href="~/AA1.3/css/webflow.css" rel="stylesheet" type="text/css">
    <link href="~/AA1.3/css/myAtlice.webflow.css" rel="stylesheet" type="text/css">
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js" type="text/javascript"></script>
    <script type="text/javascript">WebFont.load({ google: { families: ["Montserrat:100,100italic,200,200italic,300,300italic,400,400italic,500,500italic,600,600italic,700,700italic,800,800italic,900,900italic", "Varela:400", "Varela Round:400", "DM Sans:regular,500,700"] } });</script>
    <!-- [if lt IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.min.js" type="text/javascript"></script><![endif] -->
    <script type="text/javascript">!function (o, c) { var n = c.documentElement, t = " w-mod-"; n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch") }(window, document);</script>
    <link href="../AA1.3/images/favicon.png" rel="shortcut icon" type="image/x-icon">
    <link href="../AA1.3/icons/mobileIcon.png" rel="apple-touch-icon">

</head>
<body>
    <div class="col-md-8">
        <div class="all-users-main-layout-container">
            <div class="app-main-layout-mask"></div>
            <div class="app-main-layout-3">


                <div class="app-main-layout-header-2 hidden">
                    <div class="app-main-layout-wrapper-2 header">

                        <div class="page-heading">
                            <a href="#" onclick="(window.print())" class="print-contact-order w-inline-block"></a>

                            <h2 class="heading-62">Order No. @Model.Order.Order.OrderNumber</h2>
                            <p>@Model.Order.Order.Name</p>
                            <div>
                                <form asp-page-handler="ChangeStatus">
                                    <input name="id" type="hidden" value="@Model.Order.Order.Id" />
                                    <select name="status">
                                        <option value="NewOrder">New Order</option>
                                        <option value="RecuritmentOrder">Recuritment Order</option>
                                        <option value="ManualOrder">Manual Order</option>
                                        <option value="PartiallyComplete">Partially Complete</option>
                                        <option value="PendingShipment">Pending Shipment</option>
                                        <option value="Shipped">Shipped</option>
                                        <option value="Activated">Activated</option>
                                        <option value="PartiallyActivated">Partially Activated</option>
                                        <option value="Incomplete">Incomplete</option>
                                        <option value="Test">Test</option>

                                    </select>
                                    <input type="submit" value="Change this shit!" />
                                </form>

                                <form asp-page-handler="AddDevice">

                                    <input name="id" type="hidden" value="@Model.Order.Order.Id" />
                                    <select name="device">
                                        <option value="CLSCDWHT">Classic Card</option>
                                        <option value="CLSTG25GRY1">Classic Tag</option>
                                        <option value="LGYCSTLGCD1">Legacy Card</option>
                                        
                                    </select>
                                    <input type="submit" value="Add a device!" />
                                </form>

                            </div>
                        </div>
                        <div class="page-tabs-menu hidden">
                            <a href="#" class="page-tab-link-2 traffic">New</a>
                            <a href="#" class="page-tab-link-2 revenue">Assigned</a>
                            <a href="#" class="page-tab-link-2 crypto">Activated</a>
                            <a href="#" class="page-tab-link-2 projects">Inactive</a>
                        </div>

                    </div>
                </div>
                <div class="app-main-layout-wrapper">
                    <div class="app-main-layout-content">
                        <div class="section">
                            <div class="row w-row">
                                <div class="col w-col w-col-6">
                                    <div class="flex-center profile-about-row">
                                        <div class="profile-info-heading-2">
                                            <strong>Order #</strong>
                                        </div>
                                        <div>@Model.Order.Order.OrderNumber</div>
                                    </div>
                                    <div class="flex-center profile-about-row">
                                        <div class="profile-info-heading-2">
                                            <strong>Date</strong>
                                        </div>
                                        <div>@Model.Order.Order.OrderRecieved</div>
                                    </div>
                                    <div class="flex-center profile-about-row">
                                        <div class="profile-info-heading-2">
                                            <strong>Name</strong>
                                        </div>
                                        <div>@Model.Order.Order.Name</div>
                                    </div>
                                    <div class="flex-center profile-about-row">
                                        <div class="profile-info-heading-2">
                                            <strong>Phone</strong>
                                        </div>
                                        <div>@Model.Order.Order.Phone</div>
                                    </div>
                                    <div class="flex-center profile-about-row">
                                        <div class="profile-info-heading-2">
                                            <strong>Email</strong>
                                        </div>
                                        <div>@Model.Order.Order.Email</div>
                                    </div>
                                    <div class="flex-center profile-about-row">
                                        <div class="profile-info-heading-2">
                                            <strong>Shipping Address</strong>
                                        </div>
                                        <div>
                                            @Model.Order.Order.ShipAddressLine1.
                                            <br />@Model.Order.Order.ShipAddressLine2
                                            <br />@Model.Order.Order.ShipCity @Model.Order.Order.ShipState
                                            <br />@Model.Order.Order.ShipCode
                                        </div>
                                    </div>
                                </div>
                                <div class="col w-col w-col-6">
                                    <div class="flex-center profile-about-row">
                                        <div class="profile-info-heading-2">
                                            <strong>Payment</strong>
                                        </div>
                                        <p>
                                            <strong></strong>xxxx-xxxx-xxxx-@Model.Order.Order.Last4
                                            <strong> @Model.Order.Order.Brand</strong>
                                            <strong><br></strong>
                                        </p>
                                    </div>
                                    <div class="flex-center profile-about-row">
                                        <div class="profile-info-heading-2">
                                            <strong>Items Ordered</strong>
                                        </div>
                                        @foreach (var device in Model.Order.Order.Taps)
                                        {
                                            var d = new Dictionary<string, string>
                                        {
                                        { "img", Model.Order.downloads.ElementAt(Model.Order.Order.Taps.ToList().FindIndex(x=>x.Id == device.Id)).ToString() }

                                        };
                                            <div class="item-1-block">
                                                <div id="w-node-_25fc1321-2b83-1f55-02fb-65e7023ae7af-023ae6ec">

                                                    <img src="@device.GetImage(device.Sku)" loading="lazy" sizes="100vw" height="20" srcset="@device.GetImage(device.Sku) 500w, @device.GetImage(device.Sku) 800w, @device.GetImage(device.Sku) 1013w" alt="" class="item-photo">

                                                </div>
                                                <div class="div-block-11">

                                                    <div class="item-text-box">@device.GetOfficialName(device.Sku) (@device.SNumber.Substring(0, 8))</div>

                                                </div>
                                                <div>
                                                    <div class="quantity-text-box"><a asp-page-handler="DownloadQR" asp-all-route-data="d" class="atlice-button">@Html.Raw(Model.Order.qrs.ElementAt(Model.Order.Order.Taps.ToList().FindIndex(x=>x.Id == device.Id)))</a> </div>
                                                    <br />
                                                    <a href="https://atlice.com/tap/@device.SNumber.Substring(0,8)">https://atlice.com/tap/@device.SNumber.Substring(0,8)</a>
                                                </div>
                                            </div>
                                            @if (@device.GetOfficialName(device.Sku).Contains("Custom"))
                                            {
                                                <div style="text-align:center;">
                                                    <img height="100" width="50" src="@Model.Order.Order.LogoUrl" style="padding:2px 2px 2px 2px;" />
                                                    <p>@Model.Order.Order.Comments</p>
                                                </div>

                                            }
                                        }


                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="footer-3">
                        <div class="w-row">
                            <div class="w-col w-col-6">
                                <div class="hint-4">© Copyright Atlice LLC. All rights reserved</div>
                            </div>
                            <div class="footer-right w-col w-col-6">
                                <div class="hint-4">
                                    <a href="https://webflow.com/" class="footer-link-5">Empowered by Atlice</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <div class="col-md-4">
        hi
    </div>
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=60442746f919e95d6b31b806" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="~/AA1.3/js/webflow.js" type="text/javascript"></script>
    <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->
    <script src="~/js/signalr/dist/browser/signalr.min.js"></script>
    <script src="~/lib/jquery.unobtrusive-ajax.min.js"></script>
</body>
</html>

