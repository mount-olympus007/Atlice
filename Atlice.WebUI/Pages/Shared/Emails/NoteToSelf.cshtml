<body class="body">
    <div class="email-wrapper">
        <div class="email-content-wrapper">
            <div class="row center header">
                <div class="browser-link">
                    <a href="#" class="button-small-grey">Difficulty viewing this email? View in browser.</a>
                </div>
                <div class="div-block">
                    <a href="http://atlice.com" class="brand-logo w-inline-block"><img src="https://atlice.com/atlice_emails/images/image-410.png" width="120" srcset="https://atlice.com/atlice_emails/images/image-410-p-500.png 500w, https://atlice.com/atlice_emails/images/image-410-p-800.png 800w, https://atlice.com/atlice_emails/images/image-410-p-1080.png 1080w, https://atlice.com/atlice_emails/images/image-410.png 1316w" sizes="120px" alt="" class="image-5"></a>
                </div>
            </div>
            <div class="row-dark atlice-tap note-to-self">
                <div class="direct-message-title">
                    <h4 class="text-block black"><br>Hey, you sent this note to yourself from @Model.WhosPage’s contact page at atlice.com.<br><br>Scroll down for your invite!<br></h4>
                </div>
                <div class="row-white white-box">
                    <div class="div-block-4">
                        <div class="div-block-5">
                            <img src="@Model.Image" loading="lazy" height="" srcset="@Model.Image 500w, @Model.Image 800w, @Model.Image 1080w, @Model.Image 1138w" sizes="100px" alt="" class="image-27">
                            <h3 class="atlice-tap-text-title small">@Model.WhosPage</h3>
                            @if (!string.IsNullOrEmpty(Model.Location))
                            {
                                <h3 class="atlice-tap-text-title small">@Model.Location</h3>
                            }

                        </div>
                        <div class="text-block-3"><strong class="bold-text-2">Note:</strong></div>
                        <p class="text-block-dark shaded">
                            <strong>&quot;</strong>@Model.Note<strong>&quot;</strong>
                            <a href="http://atlice.com/tap/features"></a><br>
                        </p>
                        <div class="save-as-contact-link-block">
                            <div class="div-block-20">
                                <a href="@Model.WhosVcard" class="button w-button">Save Contact</a>
                            </div>
                            <div class="text-block-8"><strong class="tap-to-save-as-contact-link">Tap to Download vCard</strong></div>
                        </div>
                    </div>
                    <a href="http://atlice.com/tap/features" class="button inside-plain-text w-button">Learn more</a>
                </div>
                <div class="invite-only-redemtion-block">
                    <div class="simple-image"><img src="https://atlice.com/atlice_emails/images/EE3E3C3D-040E-4FDB-90BF-BAE5E4C1D6DB.jpeg" srcset="https://atlice.com/atlice_emails/images/EE3E3C3D-040E-4FDB-90BF-BAE5E4C1D6DB-p-500.jpeg 500w, https://atlice.com/atlice_emails/images/EE3E3C3D-040E-4FDB-90BF-BAE5E4C1D6DB-p-800.jpeg 800w, https://atlice.com/atlice_emails/images/EE3E3C3D-040E-4FDB-90BF-BAE5E4C1D6DB-p-1600.jpeg 1600w, https://atlice.com/atlice_emails/images/EE3E3C3D-040E-4FDB-90BF-BAE5E4C1D6DB-p-2000.jpeg 2000w, https://atlice.com/atlice_emails/images/EE3E3C3D-040E-4FDB-90BF-BAE5E4C1D6DB.jpeg 2048w" sizes="(max-width: 479px) 98vw, (max-width: 767px) 99vw, 700px" alt=""></div>
                    @if (Model.PageOwnerRole == "Citizen" || Model.PageOwnerRole == "Adminis")
                    {
                        if (Model.Role == "Lead" || Model.Role == "Invited")
                        {
                            <div class="simple-content">
                                <div class="row-content center bottom-space">
                                    <h1 class="heading-10">Get Invite-Only Access at No Charge<br>‍<br></h1>
                                    <p class="dark-text">Begin contactless sharing of your <strong>contact info, blockchain addresses,</strong> <strong>websites</strong>, <strong>social media</strong><strong></strong>, <strong>shop items</strong>, <strong>payment services</strong> and more!</p>
                                    <a href="@Model.Reedem" class="button-large-2 atlice-tap black-border w-button">Get Access</a>
                                    <p class="dark-text humble-brag">No Mobile App or Credit Card Required</p>
                                </div>
                                <div>
                                    <p class="text-small text-light">*No monthly subscription required. QR code included for sharing contact pages. <br>*Atlice Tap cloud standard features are included. Contactless products are not included. *Products will be made available to purchase and are not necessary for sharing contact pages.</p>
                                </div>
                            </div>
                        }
                    }

                </div>
            </div>
        </div>
        <div class="row footer atlice-tap">
            <div class="social-links center text-small">
                <a href="https://soskyhighmedia.com" target="_blank" class="social-icon w-inline-block"><img src="https://atlice.com/atlice_emails/images/SOSKYHIGH-Media-LOGO.png" width="98" alt=""></a>
                <a href="https://atlice.com" class="social-icon w-inline-block"><img src="https://atlice.com/atlice_emails/images/atlice-tap-nfc-tag-4.png" width="60" srcset="https://atlice.com/atlice_emails/images/atlice-tap-nfc-tag-4-p-500.png 500w, https://atlice.com/atlice_emails/images/atlice-tap-nfc-tag-4-p-800.png 800w, https://atlice.com/atlice_emails/images/atlice-tap-nfc-tag-4-p-1080.png 1080w, https://atlice.com/atlice_emails/images/atlice-tap-nfc-tag-4-p-1600.png 1600w, https://atlice.com/atlice_emails/images/atlice-tap-nfc-tag-4.png 1800w" sizes="60px" alt=""></a>
