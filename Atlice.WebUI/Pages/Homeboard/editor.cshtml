@model Atlice.Domain.Entities.ContactPage
@{
    Layout = null;
}



<div id="contacttab" data-w-tab="Contact Card" class="tab-pane-contact-card w-tab-pane w--tab-active">

    <div class="contact-page-editor-title business-page-editor wf-section">
        <div class="text-block-49 page-editor">@Model.PageType</div>
        @if (Model.PageType == Domain.Entities.PageType.Personal)
        {
            <div class="subheading-text-block page-editor">For fun</div>


        }
        @if (Model.PageType == Domain.Entities.PageType.Business)
        {
            <div class="subheading-text-block page-editor">For your own business or brand</div>


        }
        @if (Model.PageType == Domain.Entities.PageType.Professional)
        {
            <div class="subheading-text-block page-editor">For your job or a team</div>


        }
    </div>
    <a onclick="javascript:{$('.image-cropper').attr('style','display:block;'); document.getElementById('imageidholder').setAttribute('value','@Model.Id');}" data-w-id="8dd5b4d7-9985-b9a4-6871-3b44fa2f6f59" href="#" style="display:none;" class="edit-profile-bg w-inline-block hidden"></a>
