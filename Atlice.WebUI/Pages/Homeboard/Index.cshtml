    <script>
        var sendMessage = document.getElementById("sendMessage");
        var sendButton = document.getElementById("sendButton");
        var chatLog = document.getElementById("chatLog");
        var socket;
        var scheme = document.location.protocol === "https:" ? "wss" : "ws";
        var port = document.location.port ? (":" + document.location.port) : "";
        var connectionUrl = scheme + "://" + document.location.hostname + port + "/ws?username=";

        function updateState() {
            function disable() {
                sendMessage.disabled = true;
                sendButton.disabled = true;
            }
            function enable() {
                sendMessage.disabled = false;
                sendButton.disabled = false;
            }


            if (!socket) {
                disable();
            } else {
                switch (socket.readyState) {
                    case WebSocket.CLOSED:
                        disable();
                        break;
                    case WebSocket.CLOSING:
                        disable();
                        break;
                    case WebSocket.CONNECTING:
                        disable();
                        break;
                    case WebSocket.OPEN:
                        enable();
                        break;
                    default:
                        disable();
                        break;
                }
            }
        }

        sendButton.onclick = function () {

            var data = sendMessage.value;

            var message = {};
            message.Type = "chat";
            message.Sender = '@User.Identity.Name';
            message.Content = data;
            message.Receiver = "Admin";
            message.IsPrivate = true;
            console.log(JSON.stringify(message));

            socket.send(JSON.stringify(message));

            sendMessage.value = "";
        };

        $(document).ready(function () {

            socket = new WebSocket(connectionUrl + '@User.Identity.Name');
            socket.onopen = function (event) {
                updateState();

                console.log("socket opened", event);
                var message = {};
                message.Type = "connection";
                message.Sender = '@User.Identity.Name';
                message.Content = "Ping";
                message.Receiver = "Admin";
                message.IsPrivate = true;
                console.log(JSON.stringify(message));

                socket.send(JSON.stringify(message));

                sendMessage.value = "";
            };
            socket.onclose = function (event) {
                updateState();
                chatLog.innerHTML += '<p>Connection closed: ' + htmlEscape(event.reason) + '</p>';
            };
            socket.onerror = updateState;
            socket.onmessage = function (event) {
                console.log(event.data);
                var receivedMessage = JSON.parse(event.data);

                if (receivedMessage.Content) {
                    if (receivedMessage.Content.includes("Is online")) {
                        document.getElementById("supportStatus").setAttribute("class", "online-status on");
                    }

                    if (receivedMessage.Sender == "Admin") {
                        alert();
                        var li = document.createElement("div");
                        li.setAttribute("class", "col-md-12");
                        li.textContent = receivedMessage.Content;

                        var anchor = document.createElement("a");
