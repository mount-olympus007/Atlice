                                            <span class="checkbox-label-8 w-form-label" for="checkbox-15"></span>

                                        }
                                        else
                                        {
                                            <div class="w-checkbox-input w-checkbox-input--inputType-custom switcher-button-checkbox"></div>
                                            <input onclick="fetch('/homeboard/Index?handler=savetaplinkstate&value='+this.previousElementSibling.getAttribute('class')+'&id='+this.id+'&name='+this.getAttribute('name')).then(x => x.json()).then(y=> document.getElementById('stcUpdate').innerHTML = y.status)" type="checkbox" name="VPreview" asp-for="@link.VPreview" id="@link.Id" data-name="Checkbox 15" style="opacity:0;position:absolute;z-index:-1">
                                            <span class="checkbox-label-8 w-form-label" for="checkbox-15"></span>

                                        }
                                    </label>
                                </div>
                                <div class="card-setting-text">@link.SocialProvider</div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="w-form-done">
                    <div>Thank you! Your submission has been received!</div>
                </div>
                <div class="w-form-fail">
                    <div>Oops! Something went wrong while submitting the form.</div>
