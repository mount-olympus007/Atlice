                                        <span class="checkbox-label-8 w-form-label" for="checkbox-15"></span>
                                    }

                                </label>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="friend-item contact">
                    <div class="friend-photo-container">
                        <img width="50" src="../images/email-mail-inbox-message.png" alt="" class="friend-photo">
                        @if (Model.EmailPublished)
                        {
                            <div class="online-status on"></div>

                        }
                        else
                        {
                            <div class="online-status off"></div>

                        }
                    </div><input onblur="fetch('/homeboard/Index?handler=savepreviewstate&value='+this.value+'&id='+this.id+'&name='+this.getAttribute('name')).then(x => x.json()).then(y=> document.getElementById('cardStatus').innerHTML = y.status)" type="email" class="input-2 contact w-input" maxlength="256" asp-for="Email" name="Email" data-name="Email" placeholder="Email" id="@Model.Id">
                    <div class="card-setting-switcher">
                        <label class="w-checkbox switcher-wrapper-checkbox">
                            @if (Model.EmailPreview)
                            {
                                <div class="w-checkbox-input w-checkbox-input--inputType-custom switcher-button-checkbox w--redirected-checked"></div>
                                <input onclick="fetch('/homeboard/Index?handler=savepreviewstate&value='+this.previousElementSibling.getAttribute('class')+'&id='+this.id+'&name='+this.getAttribute('name')).then(x => x.json()).then(y=> document.getElementById('cardStatus').innerHTML = y.status)" type="checkbox" asp-for="EmailPreview" name="EmailPreview" id="@Model.Id" checked="checked" style="opacity:0;position:absolute;z-index:-1">
                                <span class="checkbox-label-8 w-form-label" for="checkbox-15"></span>
                            }
                            else
                            {
                                <div class="w-checkbox-input w-checkbox-input--inputType-custom switcher-button-checkbox"></div>
                                <input onclick="fetch('/homeboard/Index?handler=savepreviewstate&value='+this.previousElementSibling.getAttribute('class')+'&id='+this.id+'&name='+this.getAttribute('name')).then(x => x.json()).then(y=> document.getElementById('cardStatus').innerHTML = y.status)" type="checkbox" asp-for="EmailPreview" name="EmailPreview" id="@Model.Id" style="opacity:0;position:absolute;z-index:-1">
                                <span class="checkbox-label-8 w-form-label" for="checkbox-15"></span>
                            }
                        </label>
                    </div>
                </div>
                <div class="friend-item contact">
                    <div class="friend-photo-container">
                        <img width="50" sizes="100vw" src="../images/c5305453-fe7a-49eb-8cc6-8f1b9f7a2ea6.png" srcset="../images/c5305453-fe7a-49eb-8cc6-8f1b9f7a2ea6-p-500.png 500w, ../images/c5305453-fe7a-49eb-8cc6-8f1b9f7a2ea6.png 501w" alt="" class="friend-photo">
                        @if (Model.WebsitePublished)
                        {
                            <div class="online-status on"></div>

                        }
                        else
                        {
                            <div class="online-status off"></div>

                        }
                    </div><input onblur="javascript:{var string = this.value;if (!~string.indexOf('http') && string != '') {string = 'https://' + string;} this.value=string; fetch('/homeboard/Index?handler=savepreviewstate&value='+string+'&id='+this.id+'&name='+this.getAttribute('name')).then(x => x.json()).then(y=> document.getElementById('cardStatus').innerHTML = y.status)}" type="url" class="input-2 contact w-input" maxlength="256" asp-for="Website" name="Website" data-name="Website Link 2" placeholder="Website Link (Use Https://…)" id="@Model.Id">
                    <div class="card-setting-switcher">
                        <label class="w-checkbox switcher-wrapper-checkbox">
                            @if (Model.WebsitePreview)
                            {
                                <div class="w-checkbox-input w-checkbox-input--inputType-custom switcher-button-checkbox w--redirected-checked"></div>
                                <input onclick="fetch('/homeboard/Index?handler=savepreviewstate&value='+this.previousElementSibling.getAttribute('class')+'&id='+this.id+'&name='+this.getAttribute('name')).then(x => x.json()).then(y=> document.getElementById('cardStatus').innerHTML = y.status)" type="checkbox" asp-for="WebsitePreview" name="WebsitePreview" id="@Model.Id" checked="checked" style="opacity:0;position:absolute;z-index:-1">
                                <span class="checkbox-label-8 w-form-label" for="checkbox-15"></span>
                            }
                            else
                            {
                                <div class="w-checkbox-input w-checkbox-input--inputType-custom switcher-button-checkbox"></div>
                                <input onclick="fetch('/homeboard/Index?handler=savepreviewstate&value='+this.previousElementSibling.getAttribute('class')+'&id='+this.id+'&name='+this.getAttribute('name')).then(x => x.json()).then(y=> document.getElementById('cardStatus').innerHTML = y.status)" type="checkbox" asp-for="WebsitePreview" name="WebsitePreview" id="@Model.Id" style="opacity:0;position:absolute;z-index:-1">
                                <span class="checkbox-label-8 w-form-label" for="checkbox-15"></span>
                            }
                        </label>
                    </div>
                </div>
                <div class="friend-item contact">
                    <div class="friend-photo-container">
                        <img width="50" src="../images/location.png" alt="" class="friend-photo">
                        @if (Model.LocationPublished)
                        {
                            <div class="online-status on"></div>

                        }
                        else
                        {
                            <div class="online-status off"></div>

                        }
                    </div><input onblur="fetch('/homeboard/Index?handler=savepreviewstate&value='+this.value+'&id='+this.id+'&name='+this.getAttribute('name')).then(x => x.json()).then(y=> document.getElementById('cardStatus').innerHTML = y.status)" type="text" class="input-2 contact w-input" maxlength="256" asp-for="Location" name="Location" data-name="" placeholder="Location" id="@Model.Id">
                    <div class="card-setting-switcher">
                        <label class="w-checkbox switcher-wrapper-checkbox">
                            @if (Model.LocationPreview)
                            {
                                <div class="w-checkbox-input w-checkbox-input--inputType-custom switcher-button-checkbox w--redirected-checked"></div>
                                <input onclick="fetch('/homeboard/Index?handler=savepreviewstate&value='+this.previousElementSibling.getAttribute('class')+'&id='+this.id+'&name='+this.getAttribute('name')).then(x => x.json()).then(y=> document.getElementById('cardStatus').innerHTML = y.status)" type="checkbox" asp-for="LocationPreview" name="LocationPreview" id="@Model.Id" checked="checked" style="opacity:0;position:absolute;z-index:-1">
                                <span class="checkbox-label-8 w-form-label" for="checkbox-15"></span>
                            }
                            else
                            {
                                <div class="w-checkbox-input w-checkbox-input--inputType-custom switcher-button-checkbox"></div>
                                <input onclick="fetch('/homeboard/Index?handler=savepreviewstate&value='+this.previousElementSibling.getAttribute('class')+'&id='+this.id+'&name='+this.getAttribute('name')).then(x => x.json()).then(y=> document.getElementById('cardStatus').innerHTML = y.status)" type="checkbox" asp-for="LocationPreview" name="LocationPreview" id="@Model.Id" style="opacity:0;position:absolute;z-index:-1">
                                <span class="checkbox-label-8 w-form-label" for="checkbox-15"></span>
                            }
                        </label>
                    </div>
                </div>
                <div class="form-group-2"><label for="Location-4" class="label hidden">Location</label></div>
            </form>
            <div class="success-message-3 w-form-done">
                <div>Saved!</div>
            </div>
            <div class="error-message-3 w-form-fail">
                <div>Oops! Something went wrong while submitting the form.</div>
            </div>
        </div>
        <div class="key-block">
            <div class="friend-item key-2">
                <div class="friend-photo-container">
                    <div class="switcher-button-checkbox green"></div>
                </div>
                <p class="paragraph-33 key-green">= Stage and Preview</p>
            </div>
            <div class="friend-item key">
                <div class="friend-photo-container"><img src="../images/online-status-dot.png" width="50" alt="" class="friend-photo key"></div>
                <p class="paragraph-33 key-green">= Published to Page</p>
            </div>
        </div>
    </div>
</div>
<div id="linktab" data-w-tab="Tap Links" class="w-tab-pane">
    <div class="tap-links-section wf-section">
        <div class="div-block-18 hidden">
            <div class="page-editor-heading-block">
                <div class="page-editor-title-block">
                    <div class="profile-info-heading"><strong>Flex</strong></div>
                    <img onclick="javascript:{if(document.getElementById('flexInfo').getAttribute('style') == null){document.getElementById('flexInfo').setAttribute('style','display:none;')}else{document.getElementById('flexInfo').removeAttribute('style')}}" src="../images/info-information-detail-learn-more.png" loading="lazy" data-w-id="dbcda0fa-eb38-4443-285f-9ce7de6ca552" alt="" class="image-72">
