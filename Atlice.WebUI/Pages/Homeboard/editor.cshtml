                                    @if (Model.SmsPreview)
                                    {
                                        <div class="w-checkbox-input w-checkbox-input--inputType-custom switcher-button-checkbox w--redirected-checked"></div>
                                        <input onclick="fetch('/homeboard/Index?handler=savepreviewstate&value='+this.previousElementSibling.getAttribute('class')+'&id='+this.id+'&name='+this.getAttribute('name')).then(x => x.json()).then(y=> document.getElementById('cardStatus').innerHTML = y.status)" type="checkbox" asp-for="SmsPreview" name="SmsPreview" id="@Model.Id" checked="checked" style="opacity:0;position:absolute;z-index:-1">
                                        <span class="checkbox-label-8 w-form-label" for="checkbox-15"></span>
                                    }
                                    else
                                    {
                                        <div class="w-checkbox-input w-checkbox-input--inputType-custom switcher-button-checkbox"></div>
