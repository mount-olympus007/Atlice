@page
@model Atlice.WebUI.Pages.Admin.AdminNotesModel
@{
    Layout = "_AdminLayout";

}
<div class="app-main-layout-header">
    <div class="app-main-layout-wrapper header">
        <div class="page-heading with-button">
            <h2 class="heading-62">Admin Notes</h2>
            <p>Device assignment and activation management</p>
            <div>
                Pages:
                @for (int i = 0; i < Model.NoteModels.Count(); i += 100)
                {
                    int pageNo = i / 100;
                    <a href="/admin/adminnotes/@pageNo">@pageNo</a>
                }
            </div>
            <div class="page-title-button">
                <a href="/admin/fullfillment" data-w-id="12a5f6bc-13d8-b5ce-88d8-d43504309c8e" class="button atlice-button-primary w-button">Fulfillment</a>
            </div>
        </div>
        <div class="page-tabs-menu hidden">
            <a href="#" class="page-tab-link price-plans">Atlice Tap</a>
            <a href="../AA1.3/payments/payments-variant-2-manage-subscription.html" class="page-tab-link subscription">Mastergrind Tap</a>
            <a href="../AA1.3/payments/payments-variant-3-price-plans.html" class="page-tab-link price-plans">FutureActive</a>
            <a href="../AA1.3/payments/payments-variant-4-receipt.html" class="page-tab-link receipt">Beta</a>
        </div>
    </div>
</div>
<div class="app-main-layout-wrapper">
