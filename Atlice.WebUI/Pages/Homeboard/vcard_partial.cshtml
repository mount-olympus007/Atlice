                                        {
                                            <div class="w-checkbox-input w-checkbox-input--inputType-custom switcher-button-checkbox"></div>
                                            <input onclick="fetch('/homeboard/Index?handler=savetaplinkstate&value='+this.previousElementSibling.getAttribute('class')+'&id='+this.id+'&name='+this.getAttribute('name')).then(x => x.json()).then(y=> document.getElementById('stcUpdate').innerHTML = y.status)" type="checkbox" name="ShowPreview" asp-for="@link.VPreview" id="@link.Id" data-name="Checkbox 15" style="opacity:0;position:absolute;z-index:-1">
