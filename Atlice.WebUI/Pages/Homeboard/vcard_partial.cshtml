            </div>
            <div class="w-form-fail">
                <div>Oops! Something went wrong while submitting the form.</div>
            </div>
        </div>
    </div>

    @foreach (var link in Model.TapLinks.Where(x => x.LinkType == Domain.Entities.LinkType.Connect || x.LinkType == Domain.Entities.LinkType.Pay).Take(10))
    {
        <div class="friend-item">
            <div class="friend-photo-container">
